{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "vue/require-default-prop": "warn"
  }
} 